{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "[javascript][typescript][vue]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "workbench.colorCustomizations": {
    "commandCenter.border": "#e7e7e7",
    "titleBar.activeBackground": "#1F6DD3",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1F6DD3",
    "titleBar.inactiveForeground": "#e7e7e7",
    "activityBar.activeBackground": "#1F6DD3",
    "activityBar.background": "#1F6DD3",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#2b0317",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#1F6DD3",
    "statusBar.background": "#1F6DD3",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#ffffff25",
    "statusBarItem.remoteBackground": "#1F6DD3",
    "statusBarItem.remoteForeground": "#e7e7e7"
  }
}
